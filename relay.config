# TOML formatted
# https://toml.io/en/v1.0.0
#
# Configuration for a single relay endpoint:
#
# [relay_name]
# proof_key = "test-key" <= Key needed for the hook to be relayed.
# endpoint = "test" <= Endpoint relay hook will listen for requests on.
# target = "https://boneheadsbakery.requestcatcher.com" <= destination for the relayed hook.
#
# As many relays as desired is possible. There is no soft limit.
#

[relay]
proof_key = "trust-me-bro" 
endpoint = "test-relay"
target = "https://boneheadsbakery.requestcatcher.com"
